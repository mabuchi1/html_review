%main.blog_warraper
  .blog_contents
    .blog_contents_box
      .blog_contents_box_top
        %p
          = @blog.title
        %figure.blog_contents_box_top_figure
          - @blog.images.each do |image|
            = image_tag(image, size: "75%x75%", class: "img")
      .blog_contents_box_bottom
        .blog_contents_box_bottom_text
          %span
            = @blog.text